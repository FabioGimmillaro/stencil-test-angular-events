{
  "name": "test-angular-events",
  "version": "0.0.1",
  "description": "Testing StencilJS 4.6.0 compatibility with Angular",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.js",
  "es2017": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/stencil-starter-project-name/stencil-starter-project-name.esm.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/ionic-team/stencil-component-starter.git"
  },
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "build:reproduce": "npm run build && npm pack && cd .. && cd angular-workspace && npx -p @angular/cli ng build component-library && cd dist/component-library && npm pack"
  },
  "dependencies": {
    "@stencil/core": "^4.6.0"
  },
  "devDependencies": {
    "@stencil/angular-output-target": "^0.8.3",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.11",
    "jest": "^27.5.1",
    "jest-cli": "^27.5.1",
    "puppeteer": "21.1.1"
  },
  "license": "MIT"
}
